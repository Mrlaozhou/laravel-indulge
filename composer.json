{
    "name": "mrlaozhou/indulge",
    "description": "Database field extension",
    "keywords": ["field", "laravel", "indulge", "mrlaozhou", "extension"],
    "require": {
        "php": ">= 7.0"
    },
    "authors": [
        {
            "name": "Mrlaozhou",
            "email": "zgsself@163.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "~7.3",
        "laravel/framework": "5.6.*"
    },
    "autoload": {
        "psr-4": {
            "Mrlaozhou\\Indulge\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mrlaozhou\\Indulge\\LaravelIndulgeServiceProvider"
            ],
            "aliases": {
                "IndulgeOption": "Mrlaozhou\\Indulge\\Facades\\IndulgeOption"
            }
        }
    }
}
